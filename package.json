{
  "name": "cjr",
  "description": "Runs jobs in linux containers",
  "version": "0.2.0",
  "author": "buvoli",
  "bin": {
    "penguin": "./bin/run"
  },
  "bugs": "https://github.com/buvoli/cjr/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "@types/fs-extra": "^8.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/js-yaml": "^3.12.1",
    "@types/shell-quote": "^1.6.1",
    "ajv": "^6.10.2",
    "chalk": "^3.0.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.3",
    "js-yaml": "^3.13.1",
    "shell-quote": "^1.7.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/node": "^10.17.9",
    "globby": "^10.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/buvoli/cjr",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cjr",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "config": {
        "description": "View and change the cli configuration."
      },
      "stack": {
        "description": "Build, list, and delete images cooresponding to a stack."
      },
      "job": {
        "description": "View and manage local jobs."
      },
      "rjob": {
        "description": "View and manage remote jobs."
      },
      "remote": {
        "description": "View and manage remote resources."
      },
      "jupyter": {
        "description": "Start and stop Jupyter lab and Jupyter notebook."
      },
      "help": {
        "description": "Display help for cjr."
      }
    }
  },
  "repository": "https://github.com/buvoli/cjr",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
